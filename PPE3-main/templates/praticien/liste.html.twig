{% extends "base.html.twig" %}

{% block body %}

    <h1 class="text-center mt-5">Affichage des praticiens</h1>
    <table class="content-table">
        <thead>
            <tr class="bg-dark">
                <th>Nom</th>
                <th>Pr√©nom</th>
                <th>Adresse</th>
                <th>Code Postal</th>
                <th>Ville</th>
                <th>Type</th>
            </tr>
        </thead>
        <tbody>
            {% for practicien in liste_praticiens %}
                <tr>
                    <td>{{ practicien.nom }}</td>
                    <td>{{ practicien.prenom }}</td>
                    <td>{{ practicien.adresse }}</td>
                    <td>{{ practicien.CodePostal }}</td>
                    <td>{{ practicien.ville }}</td>
                    <td>{{ practicien.type }}</td>
                </tr>
            {% endfor %}
        </tbody>
    </table>

{% endblock %}